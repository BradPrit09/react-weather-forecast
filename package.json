{
  "name": "react-weather-app",
  "version": "1.0.0",
  "main": "index.js",
  "description": "React's weather forecast application using OpenWeatherMap API",
  "author": "Ivan Turashov",
  "scripts": {
    "preinstall": "rm -rf node_modules && npm cache clean --force",
