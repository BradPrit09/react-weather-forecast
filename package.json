    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "react-swipeable": "^4.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.10",
    "html-webpack-plugin": "^3.0.4",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.4"
